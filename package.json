    "url": "git+https://github.com/bndynet/dialog-themes.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "checkbox",
    "radio"
  ],
  "author": "Bendy Zhang <zb@bndy.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bndynet/pretty-check/issues"
  },
  "homepage": "https://github.com/bndynet/pretty-check#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "lite-server": "^2.4.0",
    "node-sass": "^4.12.0"
  },
  "dependencies": {}
}
