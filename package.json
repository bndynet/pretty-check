  "description": "pretty checkbox and radio",
  "main": "index.js",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "start": "concurrently \"npm run build && npm run watch:build\" \"npm run server\"",
    "server": "lite-server -c ./bs-config.json",
    "build": "node-sass src -o dist",
    "watch:build": "node-sass src -wo dist",
